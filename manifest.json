{
    "manifest_version": 3,
    "name": "LinkedIn Company Insights",
    "version": "1.0",
    "description": "Display company insights on LinkedIn profile pages",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["*://*.linkedin.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }